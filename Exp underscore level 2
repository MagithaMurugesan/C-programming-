#include<stdio.h>
int main()
{
int phy,chem,bio,mat,com,tot,per;
scanf("%d%d%d%d%d",&phy,&chem,&bio,&mat,&com);
tot=phy+chem+bio+mat+com;
per=tot/5;
{
printf("per:%d\n",per);
}
if(per>=90)
{
printf("grade A");
}
else if (per>=80)
{
printf("grade B");
}
else if (per>=70)
{
printf("grade C");
}
else if(per>=60)
{
printf("grade D");
}
else if(per>=40)
{
printf("grade E");
}
else if(per<=40)
{
printf("grade F");
}}
